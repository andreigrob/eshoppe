{
    "name": "eshoppe",
    "version": "1.0.0",
    "description": "eCommerce web app",
    "main": "app.js",
    "type": "module",
    "engines": {
        "node": "18.19.1"
    },
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon -I -q node app.js 2>&1 >> log.out & livereload -e 'ejs,js,html,css,scss' -x 'out,w3s-dynamic-storage/,node_modules/' -w 2000",
        "admin:createadmin": "run-func util/admin.js createAdministratorUser",
        "admin:deleteadmin": "run-func util/admin.js deleteAdministratorUser"
    },
    "devDependencies": {
        "nodemon": "latest"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "better-sqlite3": "^7.6.2",
        "better-sqlite3-session-store": "^0.1.0",
        "compression": "^1.7.4",
        "connect-flash": "^0.1.1",
        "dotenv": "^16.0.3",
        "ejs": "^3.0.1",
        "express": "^4.18.2",
        "express-handlebars": "^7",
        "express-session": "^1.17.1",
        "express-validator": "^6.4.1",
        "helmet": "^3.23.3",
        "livereload": "^0.9.3",
        "moment": "^2.27.0",
        "morgan": "^1.10.0",
        "multer": "latest",
        "multer-s3": "latest",
        "node-fetch": "^2.6.7",
        "nodemailer": "latest",
        "serve-favicon": "^2.5.0",
        "stripe": "^8.55.0",
        "uuid": "latest",
        "run-func": "^3.0.0",
        "prompt-sync": "^4.2.0"
    }
}